{
  "private": true,
  "name": "victor",
  "version": "0.2.6",
  "description": "A JavaScript 2D vector library for common vector operations",
  "keywords": [
    "vector",
    "2d"
  ],
  "main": "build/node/index.js",
  "jsnext:main": "build/es2015/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/maxkueng/victor.git"
  },
  "scripts": {
    "test": "npm-run-all flow lint mocha",
    "mocha": "cross-env BABEL_ENV=commonjs mocha --compilers js:babel-register --recursive test/*.js test/lib/*.js",
    "test:watch": "npm run mocha -- --watch",
    "clean": "rimraf ./build",
    "bundle": "cross-env BABEL_ENV=commonjs node ./script/bundle.js",
    "build": "npm-run-all -s clean bundle",
    "flow": "flow check",
    "lint": "eslint src/ script/ test/",
    "fix": "eslint --fix src/ script/ test/",
    "authors": "authors"
  },
  "homepage": "http://victorjs.org/",
  "author": "Max Kueng <me@maxkueng.com> (http://maxkueng.com/)",
  "contributors": [
    "Max Kueng (http://maxkueng.com/)",
    "George Crabtree (http://georgecrabtree.com/)"
  ],
  "license": "MIT",
  "devDependencies": {
    "authors": "0.0.4",
    "babel-eslint": "^7.1.1",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2015-rollup": "^1.2.0",
    "babel-register": "^6.18.0",
    "bluebird": "^3.4.6",
    "chai": "^3.5.0",
    "cross-env": "^3.1.3",
    "eslint": "3.12.0",
    "eslint-config-airbnb-base": "^10.0.1",
    "eslint-plugin-flowtype": "^2.29.1",
    "eslint-plugin-import": "^2.2.0",
    "flow-bin": "^0.36.0",
    "mocha": "^3.2.0",
    "npm-run-all": "^3.1.2",
    "rimraf": "^2.5.4",
    "rollup": "^0.36.4",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-flow": "1.1.1",
    "rollup-plugin-json": "^2.0.2",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-uglify": "^1.0.1",
    "uglify-js": "^2.7.5"
  }
}
